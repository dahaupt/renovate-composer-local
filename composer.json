    "typo3/cms-extensionmanager": "10.4.7",
    "typo3/cms-filelist": "10.4.7",
    "typo3/cms-fluid": "10.4.7",
    "typo3/cms-frontend": "10.4.7",
    "typo3/cms-install": "10.4.7",
    "typo3/cms-recordlist": "10.4.7"
  }
}
